<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="0">

        <!-- Base Data for Tipos de Proceso (loaded on every update) -->
        <record id="tipo_proceso_civil" model="tipo.proceso">
            <field name="name">CIVIL</field>
            <field name="codigo">CVL</field>
            <field name="description">Procesos civiles</field>
        </record>

        <record id="tipo_proceso_laboral_beneficios" model="tipo.proceso">
            <field name="name">LABORAL/BENEFICIOS SOCIALES</field>
            <field name="codigo">LAB-BS</field>
            <field name="description">Procesos laborales por beneficios sociales</field>
        </record>

        <record id="tipo_proceso_laboral_infraccion" model="tipo.proceso">
            <field name="name">LABORAL/INFRACCIÓN LEYES SOCIALES</field>
            <field name="codigo">LAB-ILS</field>
            <field name="description">Procesos por infracción de leyes sociales</field>
        </record>

        <record id="tipo_proceso_amparo" model="tipo.proceso">
            <field name="name">AMPARO CONSTITUCIONAL</field>
            <field name="codigo">AMP</field>
            <field name="description">Procesos de amparo constitucional</field>
        </record>

        <record id="tipo_proceso_penal_estafa" model="tipo.proceso">
            <field name="name">PENAL - Estafa agravada</field>
            <field name="codigo">PEN-EST</field>
            <field name="description">Procesos penales por estafa agravada</field>
        </record>

        <record id="tipo_proceso_penal_deberes" model="tipo.proceso">
            <field name="name">PENAL - INCUMPLIMIENTO DE DEBERES</field>
            <field name="codigo">PEN-IDB</field>
            <field name="description">Procesos penales por incumplimiento de deberes</field>
        </record>

        <record id="tipo_proceso_penal_salud" model="tipo.proceso">
            <field name="name">PENAL - DELITOS CONTRA LA SALUD</field>
            <field name="codigo">PEN-DCS</field>
            <field name="description">Procesos penales por delitos contra la salud</field>
        </record>

        <record id="tipo_proceso_conciliacion" model="tipo.proceso">
            <field name="name">CONCILIACIÓN CIVIL</field>
            <field name="codigo">CON-CVL</field>
            <field name="description">Procesos de conciliación civil</field>
        </record>

        <!-- Base Data for Ciudades -->
        <record id="ciudad_sucre" model="ciudad.ciudad">
            <field name="name">Sucre</field>
            <field name="codigo">SUC</field>
            <field name="description">Ciudad de Sucre</field>
        </record>

        <record id="ciudad_lapaz" model="ciudad.ciudad">
            <field name="name">La Paz</field>
            <field name="codigo">LPZ</field>
            <field name="description">Ciudad de La Paz</field>
        </record>

        <record id="ciudad_cochabamba" model="ciudad.ciudad">
            <field name="name">Cochabamba</field>
            <field name="codigo">CBB</field>
            <field name="description">Ciudad de Cochabamba</field>
        </record>

    </data>
</odoo>
